struct Text {
    content: str;
}

proto Text {
    fun draw(self) {
        println("Draw text {}", self.content);
    }

    fun update(self, dt: f32) {
        println("Update text {} with dt={}", self.content, dt);
    }
}

fun process_draw(t: Text) {
    t.draw();
}

fun process_update(t: Text) {
    t.update(0.016);
}

fun main() {
    var t: Text = Text { content: "Hello Proto!" };

    process_draw(t);
    process_update(t);
}
